<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rentbuysell">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Url" />
        <activity android:name=".user_profile" />
        <activity android:name=".editprofile" />
        <activity android:name=".messageInterface" />
        <activity android:name=".Chat" />
        <activity android:name=".Buy" />
        <activity android:name=".User_Description" />
        <activity android:name=".sample" />
        <activity android:name=".forgot_password" />
        <activity android:name=".SignUp" />
        <activity android:name=".signIn" />
        <activity android:name=".description" />
        <activity android:name=".Swap_Item_List" />
        <activity android:name=".ListGiveAwayItems" />
        <activity android:name=".GiveAway" />
        <activity android:name=".AboutUs" />
        <activity android:name=".Swap" />
        <activity android:name=".Rent" />
        <activity android:name=".Sell" />
        <activity android:name=".MyData" />
        <activity
            android:name=".Drawer"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.example.rentbuysell" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SplashScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name=".Notification.MyIdfirebaseservices"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/common_google_signin_btn_text_dark" />

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />
    </application>

</manifest>